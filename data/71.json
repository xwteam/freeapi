    "url": "https:\/\/api.fuli.pp.ua\/api\/img\/pic",
    "method": "GET\/POST",
    "returnFormat": "application\/json",
    "params": [
        {
            "name": "category",
            "required": true,
            "type": "string",
            "example": "fj",
            "description": "要获取的图片类型<br>游戏[yx]、美女[mn]、风景[fj]、视觉创意[cy]、明星影视[mxys]、汽车[qc]、动物[dw]、小清新[xqs]、体育[ty]、军事[js]、动漫[dm]、情感[qg]、文字[wz]<br>默认category=fj"
        }
    ],
    "example": "https:\/\/api.fuli.pp.ua\/api\/img\/pic?category=yx",
    "returnParams": [
        {
            "name": "code",
            "type": "int",
            "description": "状态码"
        },
        {
            "name": "msg",
            "type": "string",
            "description": "状态信息"
        },
        {
            "name": "data",
            "type": "string",
            "description": "请求结果数据集"
        },
        {
            "name": "debug",
            "type": "string\/array",
            "description": "调试数据"
        },
        {
            "name": "exec_time",
            "type": "float",
            "description": "执行耗时"
        },
        {
            "name": "user_ip",
            "type": "string",
            "description": "客户端IP"
        }
    ]
}