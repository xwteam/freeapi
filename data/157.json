{
    "title": "货币换算",
    "description": "官方换算接口，24小时实时货币汇率",
    "url": "https:\/\/api.fuli.pp.ua\/api\/convert\/currency",
    "method": "GET\/POST",
    "returnFormat": "application\/json",
    "params": [
        {
            "name": "type",
            "required": false,
            "type": "string",
            "example": "list",
            "description": "可空，【type=list】查询世界货币符号"
        },
        {
            "name": "money",
            "required": false,
            "type": "string",
            "example": "CNY",
            "description": "可空，为空默认查询CNY"
        }
    ],
    "example": "https:\/\/api.fuli.pp.ua\/api\/convert\/currency?type=list<br>https:\/\/api.fuli.pp.ua\/api\/convert\/currency?money=货币符号",
    "returnParams": [
        {
            "name": "code",
            "type": "int",
            "description": "状态码"
        },
        {
            "name": "msg",
            "type": "string",
            "description": "状态信息"
        },
        {
            "name": "data",
            "type": "string",
            "description": "请求结果数据集"
        },
        {
            "name": "debug",
            "type": "string\/array",
            "description": "调试数据"
        },
        {
            "name": "exec_time",
            "type": "float",
            "description": "执行耗时"
        },
        {
            "name": "user_ip",
            "type": "string",
            "description": "客户端IP"
        }
    ]
}